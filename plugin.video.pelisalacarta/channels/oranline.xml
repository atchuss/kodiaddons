<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<channel>
	<id>oranline</id>
	<name>Oranline</name>

	<active>true</active>
	<adult>false</adult>
	<language>es</language>

	<fanart>https://d18.usercdn.com/i/03860/v89p218dygxz.jpg</fanart>
	<thumbnail>http://media.tvalacarta.info/pelisalacarta/squares/oranline.png</thumbnail>
	<bannermenu>http://media.tvalacarta.info/pelisalacarta/bannermenu/oranline.png</bannermenu>

	<update_url>https://raw.githubusercontent.com/tvalacarta/pelisalacarta/master/python/main-classic/channels/</update_url>
	<version>5</version>
	<date>08/07/2016</date>
	<changes>Correcciones y adaptacion a la nueva version</changes>

	<categories>
		<category>latino</category>
		<category>movie</category>
		<category>documentary</category>
	</categories>

	<settings>
		<id>include_in_global_search</id>
		<type>bool</type>
		<label>Incluir en busqueda global</label>
		<default>false</default>
		<enabled>true</enabled>
		<visible>true</visible>
	</settings>
	<settings>
		<id>include_in_newest_peliculas</id>
		<type>bool</type>
		<label>Incluir en Novedades - Películas</label>
		<default>true</default>
		<enabled>true</enabled>
		<visible>true</visible>
	</settings>
	<settings>
		<id>include_in_newest_documentales</id>
		<type>bool</type>
		<label>Incluir en Novedades - Documentales</label>
		<default>true</default>
		<enabled>true</enabled>
		<visible>true</visible>
	</settings>
	<settings>
		<id>modo_grafico</id>
		<type>bool</type>
		<label>Buscar información extra</label>
		<default>true</default>
		<enabled>true</enabled>
		<visible>true</visible>
	</settings>
	<settings>
		<id>perfil</id>
		<type>list</type>
		<label>Perfil de color</label>
		<default>0</default>
		<enabled>true</enabled>
		<visible>true</visible>
		<lvalues>Perfil 3</lvalues>
		<lvalues>Perfil 2</lvalues>
		<lvalues>Perfil 1</lvalues>
	</settings>
	<settings>
		<id>filterlanguages</id>
		<type>list</type>
		<label>Mostrar enlaces en idioma...</label>
		<default>4</default>
		<enabled>true</enabled>
		<visible>true</visible>
		<lvalues>Inglés</lvalues>
		<lvalues>VOSE</lvalues>
		<lvalues>Latino</lvalues>
		<lvalues>Español</lvalues>
		<lvalues>No filtrar</lvalues>
	</settings>
	<settings>
		<id>filterlinks</id>
		<type>list</type>
		<label>Mostrar enlaces de tipo...</label>
		<default>2</default>
		<enabled>true</enabled>
		<visible>true</visible>
		<lvalues>Solo Descarga</lvalues>
		<lvalues>Solo Online</lvalues>
		<lvalues>No filtrar</lvalues>
	</settings>
	<settings>
		<id>viewmode</id>
		<type>list</type>
		<label>Elegir vista por defecto (Confluence)...</label>
		<default>2</default>
		<enabled>true</enabled>
		<visible>true</visible>
		<lvalues>Sinopsis</lvalues>
		<lvalues>Miniatura</lvalues>
		<lvalues>Lista</lvalues>
	</settings>
</channel>
